//Main function => the execution begins from here
void main() {
  
  String myName = 'Harkirat Nagra';
  bool multipleOf3, multipleOf5;
  
  //Using for loop below to iterate numbers from 1 to 100
  for (int i = 1; i <= 100; i++) {
    
    //Checking conditions and saving results here for more readibility and concise code
    multipleOf3 = i%3==0;
    multipleOf5 = i%5==0;
    
    //Using if-else-if conditions to customize results as needed
    if(multipleOf3 && multipleOf5){
      print('Budvue should consider $myName for this position\n');
    }
    else if(multipleOf3){
      print('Bud\n');
    }
    else if(multipleOf5){
      print('Vue\n');
    }
    else{
    print('$i\n');
    }
  }
}
